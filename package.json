{
    "name": "learnedlocal",
    "private": true,
    "engines": {
      "node": ">=v18.18.0"
    },
    "packageManager": "pnpm@8.1.1",
    "scripts": {
      "build": "turbo run build",
      "dev": "turbo run dev --parallel",
      "clean": "find . -name node_modules -o -name .next -o -name .expo -type d -prune | xargs rm -rf",
      "lint": "turbo run lint",
      "format": "prettier --write \"**/*.{ts,tsx,md}\""
    },
    "dependencies": {
      "@manypkg/cli": "^0.21.0",
      "@turbo/gen": "^1.10.14",
      "prettier": "^3.0.3",
      "turbo": "^1.10.14",
      "typescript": "^5.2.2"
    }
  }